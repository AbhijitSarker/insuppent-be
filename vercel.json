{
    "version": 2,
    "builds": [
        {
      "src": "src/serverless.js",
            "use": "@vercel/node"
        }
    ],
  "functions": {
    "src/serverless.js": {
      "includeFiles": "node_modules/mysql2/**"
    }
  },
    "routes": [
        {
      "src": "/(.*)",
      "dest": "src/serverless.js"
        }
    ]
}